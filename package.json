{
  "name": "card-puzzle",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:ui-gen": "tsc -p tsconfig.generator-ui.json",
    "build:ui-engine": "tsc -p tsconfig.ui-engine.json",
    "build:ui": "npm run build:ui-engine && npm run build:ui-gen",
    "test": "npm run build && node --test dist/tests/engine/*.test.js",
    "test:watch": "npm run build && node --test --watch dist/tests/engine/*.test.js"
  },
  "devDependencies": {
    "typescript": "^5.4.0"
  }
}
